@model IEnumerable<PersonsModel>


<div class="container mt-4">
    <table class="table">
        <a class="btn btn-primary" asp-page="/Create">Create</a>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Address</th>
        </tr>
        @foreach (var person in Model)
        {
            <tr>
                <td>@person.FirstName</td>
                <td>@person.LastName</td>
                <td>@person.Address</td>
            </tr>
        }
    </table>
</div>